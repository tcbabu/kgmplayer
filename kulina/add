	  if((pos >=0)&&(pos!=(item-1)) ) {
	    tpt = thy[item-1];
            if(pos < (item-1) ) {
              for(k=(item-1);k>pos;k--){ thy[k]=thy[k-1]; }
              thy[pos]= tpt;
            }
            else {
              for(k=item;k<pos;k++) {thy[k-1]=thy[k];}
              thy[pos-1]= tpt;
            }
